{
  "expo": {
    "name": "LexSee",
    "slug": "lexsee_v3_frontend",
    "version": "3.0.0",
    "orientation": "portrait",
    "icon": "./assets/LexSeeAppLogo.png",
    "newArchEnabled": false,
    "scheme": "lexsee-v3",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.leoyang-2001.lexsee-v3-frontend",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1F2734"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.leoyang_2001.lexsee_v3_frontend"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      "expo-router",

      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "869eddf8-c7bf-4c1e-aa45-1ca742707208"
      }
    }
  }
}
