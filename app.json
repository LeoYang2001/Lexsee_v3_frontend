{
  "expo": {
    "name": "lexsee_v3_frontend",
    "slug": "lexsee_v3_frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "lexsee-v3",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.leoyang-2001.lexsee-v3-frontend"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.leoyang_2001.lexsee_v3_frontend"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#1F2734",
          "image": "./assets/splashScreen.png",
          "dark": {
            "backgroundColor": "#1F2734"
          }
        }
      ],
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ]
    ]
  }
}
